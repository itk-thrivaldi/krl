<ROOT>
   <CONFIG>
      <IP_NUMBER>10.0.0.200</IP_NUMBER>   <!-- IP-number of the external socket !-->
      <PORT>49152</PORT>                  <!-- Port-number of the external socket !-->
      <PROTOCOL>UDP</PROTOCOL>            <!-- TCP or UDP, Protocol of the socket !-->
      <SENTYPE>ImFree</SENTYPE>           <!-- The name of your system send in <Sen Type="" > !-->
      <ONLYSEND>FALSE</ONLYSEND>          <!-- TRUE means the client don't expect answers. Do not send anything to robot !-->
   </CONFIG> 
   <SEND>
      <ELEMENTS>
         <ELEMENT TAG="DEF_RIst" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
         <ELEMENT TAG="DEF_RSol" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
         <ELEMENT TAG="DEF_AIPos" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
         <ELEMENT TAG="DEF_ASPos" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
      	 <ELEMENT TAG="DEF_MACur" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
         <ELEMENT TAG="DEF_Delay" TYPE="LONG" INDX="INTERNAL" UNIT="0" />
         <ELEMENT TAG="FTC.Fx" TYPE="DOUBLE" INDX="1" UNIT="0" />     
         <ELEMENT TAG="FTC.Fy" TYPE="DOUBLE" INDX="2" UNIT="0" />
         <ELEMENT TAG="FTC.Fz" TYPE="DOUBLE" INDX="3" UNIT="0" />
         <ELEMENT TAG="FTC.Mx" TYPE="DOUBLE" INDX="4" UNIT="0" />
         <ELEMENT TAG="FTC.My" TYPE="DOUBLE" INDX="5" UNIT="0" />
         <ELEMENT TAG="FTC.Mz" TYPE="DOUBLE" INDX="6" UNIT="0" />
      </ELEMENTS>
   </SEND>
   <RECEIVE>
      <ELEMENTS>
         <ELEMENT TAG="AK.A1" TYPE="DOUBLE" INDX="1" UNIT="0" HOLDON="0" />
         <ELEMENT TAG="AK.A2" TYPE="DOUBLE" INDX="2" UNIT="0" HOLDON="0" />
         <ELEMENT TAG="AK.A3" TYPE="DOUBLE" INDX="3" UNIT="0" HOLDON="0" />
         <ELEMENT TAG="AK.A4" TYPE="DOUBLE" INDX="4" UNIT="0" HOLDON="0" />
         <ELEMENT TAG="AK.A5" TYPE="DOUBLE" INDX="5" UNIT="0" HOLDON="0" />
         <ELEMENT TAG="AK.A6" TYPE="DOUBLE" INDX="6" UNIT="0" HOLDON="0" />
      </ELEMENTS>
   </RECEIVE>
</ROOT>
